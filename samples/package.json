{
  "name": "todo-app",
  "version": "0.0.1",
  "description": "Sample Todo app to demostrate DocsDD.",
  "main": "index.js",
  "author": "",
  "license": "",
  "scripts": {
    "build-docs": "node ../cli/dist/cli.js docs --input=01_getting-started/docs.ts --output=01_getting-started//docs.html",
    "watch-docs": "npm run build-docs -- --watch",
    "build-docs-tests": "node ../cli/dist/cli.js tests --target=cypress --input=01_getting-started/docs.ts --output=cypress/integration/01_getting-started/_tests.ts",
    "watch-docs-tests": "npm run build-docs-tests -- --watch",
    "test": "cypress open",
    "test:prod": "cypress run"
  },
  "devDependencies": {
    "cypress": "^9.1.1",
    "typescript": "^4.5.4"
  }
}
